{
  "functions": {
    "api/team-events.js": {
      "maxDuration": 10
    },
    "api/team-near-events.js": {
      "maxDuration": 10
    }
  },
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "/api/$1"
    },
    {
      "src": "/[^.]+",
      "dest": "/",
      "status": 200
    }
  ]
}
